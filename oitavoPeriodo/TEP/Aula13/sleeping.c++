ll nn, h, l, r, arr[size1], dp[size1][size1];

ll dpCall(ll pos, ll hour)
{
    if (pos >= nn)
        return 0;
    if (dp[pos][hour] != -1)
        return dp[pos][hour];

    ll step1 = (hour + arr[pos]) % h;
    ll step2 = (hour + arr[pos] - 1) % h;

    ll call1 = dpCall(pos + 1, step1) + (step1 >= l && step1 <= r);
    ll call2 = dpCall(pos + 1, step2) + (step2 >= l && step2 <= r);

    return dp[pos][hour] = max(call1, call2);
}

